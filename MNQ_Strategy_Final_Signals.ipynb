{"cells":[{"cell_type":"code","source":'\n# نصب کتابخانه فقط یک\u200cبار لازم است\n!pip install mplfinance\n\nimport requests\nimport pandas as pd\nimport numpy as np\nimport mplfinance as mpf\nfrom datetime import datetime, timedelta\n\n# ========== پارامترهای قابل تغییر توسط کاربر ==========\naccess_token = "توکن_خودت_را_اینجا_قرار_بده"\ncontract_id = "CON.F.US.MNQ.U25"\nstart_time = "2025-06-24T00:00:00Z"\nend_time = "2025-06-25T00:00:00Z"\nunit = 2  # Minute\nunit_number = 5\nmin_pct_distance = 0.15\nmax_pct_distance = 0.55\nrisk_reward_ratio = 1.65\n\n# ========== دریافت داده\u200cها ==========\nurl = "https://api.topstepx.com/api/History/retrieveBars"\npayload = {\n    "contractId": contract_id,\n    "live": False,\n    "startTime": start_time,\n    "endTime": end_time,\n    "unit": unit,\n    "unitNumber": unit_number,\n    "limit": 500,\n    "includePartialBar": False\n}\nheaders = {\n    "Authorization": f"Bearer {access_token}",\n    "Content-Type": "application/json",\n    "accept": "application/json"\n}\n\nresponse = requests.post(url, json=payload, headers=headers)\n\nif response.status_code != 200:\n    print("❌ خطا در دریافت داده\u200cها:", response.status_code)\nelse:\n    bars = response.json()\n    df = pd.DataFrame(bars["bars"])\n    df.rename(columns={"t": "Date", "o": "Open", "h": "High", "l": "Low", "c": "Close", "v": "Volume"}, inplace=True)\n    df["Date"] = pd.to_datetime(df["Date"])\n    df.set_index("Date", inplace=True)\n\n    # محاسبه Heikin Ashi\n    ha_close = (df["Open"] + df["High"] + df["Low"] + df["Close"]) / 4\n    ha_open = ha_close.copy()\n    for i in range(1, len(df)):\n        ha_open.iloc[i] = (ha_open.iloc[i-1] + ha_close.iloc[i-1]) / 2\n    ha_high = pd.concat([df["High"], ha_open, ha_close], axis=1).max(axis=1)\n    ha_low = pd.concat([df["Low"], ha_open, ha_close], axis=1).min(axis=1)\n\n    # محاسبه MAها\n    ema1 = df["Close"].ewm(span=9).mean()\n    ema2 = df["Close"].ewm(span=29).mean()\n    trend_ma = df["Close"].ewm(span=100).mean()\n\n    # بررسی سیگنال\u200cها\n    signal = []\n    for i in range(1, len(df)):\n        bull_cross = ema1.iloc[i-1] < ema2.iloc[i-1] and ema1.iloc[i] > ema2.iloc[i]\n        bear_cross = ema1.iloc[i-1] > ema2.iloc[i-1] and ema1.iloc[i] < ema2.iloc[i]\n        trend_up = df["Close"].iloc[i] > trend_ma.iloc[i]\n        trend_down = df["Close"].iloc[i] < trend_ma.iloc[i]\n        ha_bullish = ha_close.iloc[i] > ha_open.iloc[i]\n        ha_bearish = ha_close.iloc[i] < ha_open.iloc[i]\n\n        long_dist_ok = abs(ha_high.iloc[i] - trend_ma.iloc[i]) / trend_ma.iloc[i] * 100\n        short_dist_ok = abs(ha_low.iloc[i] - trend_ma.iloc[i]) / trend_ma.iloc[i] * 100\n\n        if bull_cross and trend_up and ha_bullish and min_pct_distance <= long_dist_ok <= max_pct_distance:\n            signal.append("BUY")\n        elif bear_cross and trend_down and ha_bearish and min_pct_distance <= short_dist_ok <= max_pct_distance:\n            signal.append("SELL")\n        else:\n            signal.append("")\n\n    signal.insert(0, "")\n    df["Signal"] = signal\n\n    # چاپ آخرین سیگنال\n    last_signal = df[df["Signal"] != ""].tail(1)\n    if not last_signal.empty:\n        print("✅ آخرین سیگنال:")\n        print(last_signal[["Open", "High", "Low", "Close", "Signal"]])\n    else:\n        print("❌ سیگنالی یافت نشد.")\n\n    # رسم چارت\n    mpf.plot(df, type=\'candle\', style=\'charles\', title="MNQ - Signal Check", volume=True)\n',"metadata":{},"outputs":[],"execution_count":null}],"metadata":{"kernelspec":{"name":"python3","display_name":"Python 3"}},"nbformat":4,"nbformat_minor":5}